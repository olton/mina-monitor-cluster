{
    "name": "@olton/mina-monitor-cluster",
    "version": "2.0.0",
    "license": "MIT",
    "scripts": {
        "client": "shx rm -rf .output/* && parcel serve ./src/index.html --out-dir .output -p 2222 ",
        "start": "shx rm -rf .output/* && parcel serve ./src/index.html --open --out-dir .output",
        "prebuild": "shx rm -rf build/*",
        "build": "parcel build ./src/index.html --out-dir build --no-minify --no-source-maps --public-url ."
    },
    "devDependencies": {
        "less": "^4.1.2",
        "parcel-bundler": "^1.12.5",
        "parcel-plugin-html-partials": "^0.0.6",
        "parcel-plugin-static-files-copy": "^2.6.0",
        "shx": "^0.3.3"
    },
    "staticFiles": {
        "staticPath": [
            "src/config.json",
            "src/.htaccess"
        ]
    },
    "browserslist": [
        "last 1 Chrome versions",
        "last 1 Firefox versions",
        "last 1 Opera versions",
        "last 1 Edge versions"
    ]
}
